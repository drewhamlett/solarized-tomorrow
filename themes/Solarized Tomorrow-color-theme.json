{
  "name": "Solarized Tomorrow",

  "tokenColors": [
    {
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "scope": ["punctuation.terminator.statement.tsx", "punctuation.terminator.statement.ts", "punctuation.terminator.expression.php"],
      "settings": {
        "foreground": "#d3ccb7"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "background": "#FDF6E3",
        "foreground": "#657B83"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "variable.other.constant.ts"],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.language",
        "variable.other",
        "meta.block.js",
        "keyword.operator.assignment",
        "meta.interface.tsx",
        "entity.name.function.tsx",
        "entity.name.type.alias.ts",
        "punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type", "meta.function-call.tsx entity.name.function.tsx"],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Function name",
      "scope": ["meta.object-literal.key"],
      "settings": {
        // "foreground": "#268BD2"
        "foreground": "#586e75"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.definition.function entity.name.function"
      ],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Variable start",
      "scope": ["punctuation.definition.variable", "entity.name.type.ts" , "entity.name.type.tsx meta.type.annotation.tsx entity.name.type"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "meta.preprocessor",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "variable.other.readwrite.tsx"],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Support.construct",
      "scope": [
        "support.function.construct",
        "keyword.other.new",
        "variable.language.this",
        "variable.other.readwrite.instance.ruby",
        "punctuation.definition.variable.ruby",
        "variable.language.this.php punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "meta.type.annotation.tsx keyword",
        "keyword.operator.class.php"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class", "meta.method-call.php variable.other.php punctuation.definition.variable.php", "variable.other.php"],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {}
    },
    {
      "name": "diff: header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "background": "#b58900",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "background": "#eee8d5",
        "fontStyle": "",
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "background": "#eee8d5",
        "fontStyle": "",
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "background": "#eee8d5",
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "meta.function.expression.ts meta.function-call.ts meta.block.ts",
        "meta.function-call.ts variable.other",
        "meta.var.expr.tsx variable.other.object.tsx",
        "meta.function.parameter.no-default.php variable.other.php",
        "meta.function.parameters.php punctuation.definition.variable.php  "
      ],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Template",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "support.type"
      ],
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Component name",
      "scope": [
        "entity.name.function.tsx"
      ],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "PHP",
      "scope": [ "punctuation.definition.variable"],
      "settings": {
        "foreground": "#586e7577"
      }
    },
    {
      "name": "Ruby Symbol",
      "scope": ["constant.language.symbol.hashkey.ruby"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "name": "Ruby Extended Class",
      "scope": ["entity.other.inherited-class.ruby"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Ruby Class",
      "scope": ["entity.name.type.class.ruby"],
      "settings": {
        "foreground": "#B58900"
      }
    },

    {
      "name": "Ruby Keyword Params",
      "scope": ["constant.other.symbol.hashkey.parameter.function.ruby"],
      "settings": {
        "foreground": "#D33682"

      }

    }



  ],
